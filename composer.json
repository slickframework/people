{
  "name": "slick/people",
  "description": "User management package",
  "minimum-stability": "dev",
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/slick/people",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "league/tactician": "^1.0",
    "league/tactician-container": "^1.0",
    "paragonie/random_compat": "^2.0",
    "ramsey/uuid": "^3.5",
    "slick/di": "^2.0",
    "slick/validator": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Slick\\People\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slick\\People\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "phpspec/phpspec": "^3.2",
    "phpunit/phpunit": "^5.7"
  }
}
